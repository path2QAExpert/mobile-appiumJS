{
  "name": "mobile-appium-js",
  "version": "1.0.0",
  "description": "Cross-platform mobile automation with Appium and WebdriverIO",
  "scripts": {
    "android": "wdio run wdio.android.conf.js",
    "ios": "wdio run wdio.ios.conf.js",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report"
  },
  "devDependencies": {
    "@wdio/cli": "^8.25.2",
    "@wdio/local-runner": "^8.25.2",
    "@wdio/mocha-framework": "^8.25.2",
    "@wdio/appium-service": "^8.25.2",
    "@wdio/spec-reporter": "^8.25.2",
    "@wdio/allure-reporter": "^8.25.2",
    "webdriverio": "^8.25.2",
    "appium": "^2.3.0"
  }
}